{
    "compilerOptions": {
      "target": "ES2020",
      "module": "ESNext",
      "moduleResolution": "Bundler",
      "strict": true,
      "skipLibCheck": true,
      "jsx": "preserve",
      "allowJs": true,
      "checkJs": false,
      "resolveJsonModule": true,
      "baseUrl": ".",
      "types": ["miniprogram-api-typings"]
    },
    "include": ["./**/*.ts", "./**/*.d.ts", "./**/*.json"]
  }
  